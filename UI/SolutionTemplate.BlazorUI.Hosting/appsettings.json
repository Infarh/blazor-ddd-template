{
  "Database": "InMemory",
  "ConnectionStrings": {
    "SqlServer": "Data Source=(localdb)\\MSSqlLocalDb;Initial Directory=SolutionTemplate.db;MultipleActiveResultSet=true;",
    "Sqlite": "Data Source=RRJ-Express2.db;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Debug",
        "Microsoft.Hosting.Lifetime": "Information",
        "System.Net.Http.HttpClient.WebStoreAPI": "Warning"
      },
      "FormatterOptions": {
        "IncludeScopes": false,
        "TimestampFormat": "HH:mm:ss:ffff\t",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*"
}
